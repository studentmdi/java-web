<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="employee.view.mainViewController2">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" text="Button" />
        </items>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </ToolBar>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button maxWidth="80.0" mnemonicParsing="false" onAction="#addBtn" prefWidth="80.0" text="Add ">
               <HBox.margin>
                  <Insets left="6.0" right="13.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#goHome" prefWidth="80.0" text="Home" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
